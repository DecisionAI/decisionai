   "source": [
    "from decisionai.simulation import Simulation\n",
    "from decisionai.variable import Variable as v\n",
    "\n",
    "fib_sim = Simulation(variables=[v('x', 'x[t-1] + x[t-2]', '1')],\n",
    "                     num_sims=2,\n",
    "                     num_steps=5\n",
    "                    )\n",
    "fib_sim.run()\n",
    "fib_sim.results"
   ]
  }
